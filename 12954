class Solution {
    public long[] solution(long x, int n) {
        
        long[] answer = new long[n];
        for(int i=1;i<n+1;i++){
            long num=0;

            for(int j=0;j<i;j++){
                num+=x;
            };
            answer[i-1]+=num;
        }
        
        
        return answer;
    }
}
